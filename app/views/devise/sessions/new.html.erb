
<div id="headerwrap">
<div class="center-block w-xxl w-auto-xs p-y-md">
    <div class="navbar">
      <div class="pull-center">
        <a class="navbar-brand"><img src="/assets/images/logo-green.svg"></a>
      </div>
    </div>
    <div class="p-a-md box-color r box-shadow-z1 text-color m-a">
    <div class="m-b text-sm">
        Sign in get started
      </div>
      


      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  
        <div class="form-group">
                <%= f.label :email %><br />
                <%= f.email_field :email, class: "form-control", autocomplete: "email" %>
         </div>
  

        <div class="form-group">
              <%= f.label :password %><br />
              <%= f.password_field :password, class: "form-control", autocomplete: "current-password" %>
        </div>

          <% if devise_mapping.rememberable? -%>
          
          <div class="form-group">
              <%= f.check_box :remember_me %>
              <%= f.label :remember_me %>
          </div>

          <% end -%>
          
          <div class="actions form-group">
              <%= f.submit "Log in", class: 'btn btn-block btn-outline-success' %>
          </div>
      <% end %>


<%= render "devise/shared/links" %>

  </div>

</div>

<br>
<br>
<br>

<br>
</div>